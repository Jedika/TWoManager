{
  "extends": "@quasar/app/tsconfig-preset",
  "compilerOptions": {
    "baseUrl": ".",
    "target": "es5",
    "noEmit": false,
    "noImplicitAny": true,
    "skipLibCheck": true,
    "noImplicitUseStrict": true,
    "allowJs": true,
    "strict": false,
    "removeComments": true,
    "moduleResolution": "node"
  }
}
